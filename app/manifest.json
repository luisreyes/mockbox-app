{
  "manifest_version": 2,
  "name": "MockBox",
  "version": "0.9.5.2",
  "description": "Prototype builder",
  "sandbox": {
    "pages": ["mockbox_prototype.html"]
  },
  "icons": { 
    "16": "icons/mockbox16.png",
    "32": "icons/mockbox32.png",
    "48": "icons/mockbox48.png", 
    "96": "icons/mockbox96.png" ,
    "128": "icons/mockbox128.png" ,
    "256": "icons/mockbox256.png"},
  "app": {
    "background": {
      "scripts": ["background.js"]
    }
  },
  "key": "",
  "permissions": [
    "identity",
    "notifications",
    {"fileSystem": ["write", "retainEntries", "directory"]},
    "storage"
  ],
  "oauth2": {
    "client_id": "71660103387-2enhlcncq54i15h66076c4fe5j5est7g.apps.googleusercontent.com",
    "scopes": [
      "profile",
      "https://www.googleapis.com/auth/drive.appdata",
      "https://www.googleapis.com/auth/drive.file"
    ]
  }
}